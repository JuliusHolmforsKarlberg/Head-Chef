﻿@using Head_Chef.Models.ViewModels
@using Head_Chef.Models.Pages
@using EPiServer.Web.Mvc.Html

@model IPageViewModel<SitePageData>


<!doctype html>
<html lang="en" data-bs-theme="auto">
<head>
    @*<script src="https://getbootstrap.com/docs/5.3/assets/js/color-modes.js"></script>*@

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="@Model.CurrentPage.MetaDescription">
    <meta name="author" content="Muntaha Abdulnabi, Oscar Enbacka, Julius Holmfors Karlberg, Per Norin, Simon Severyos Simonsson">
    @*<meta name="generator" content="Hugo 0.118.2">*@
    <meta title="@Model.CurrentPage.MetaTitle" />
    <title>Head Chef | @Model.CurrentPage.MetaTitle</title>

    <link rel="canonical" href="https://getbootstrap.com/docs/5.3/examples/carousel/">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@@docsearch/css@3">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">

    <link href="https://getbootstrap.com/docs/5.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

    <!-- Favicons -->
    <link rel="apple-touch-icon" href="https://getbootstrap.com/docs/5.3/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
    <link rel="icon" href="https://getbootstrap.com/docs/5.3/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
    <link rel="icon" href="https://getbootstrap.com/docs/5.3/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
    <link rel="manifest" href="https://getbootstrap.com/docs/5.3/assets/img/favicons/manifest.json">
    <link rel="mask-icon" href="https://getbootstrap.com/docs/5.3/assets/img/favicons/safari-pinned-tab.svg" color="#712cf9">
    @*<link rel="icon" href="https://getbootstrap.com/docs/5.3/assets/img/favicons/favicon.ico">*@
    <link rel="icon" href="~/gfx/knife-fork.svg">
    <meta name="theme-color" content="#712cf9">

    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

        @@media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }

        .b-example-divider {
            width: 100%;
            height: 3rem;
            background-color: #cff2e4;
            border: solid #cff2e4;
            border-width: 1px 0;
            box-shadow: inset 0 .5em 1.5em #cff2e4;
            , inset 0 .125em .5em #cff2e4;
        }

        .b-example-vr {
            flex-shrink: 0;
            width: 1.5rem;
            height: 100vh;
        }

        .bi {
            vertical-align: -.125em;
            fill: currentColor;
        }

        .nav-scroller {
            position: relative;
            z-index: 2;
            height: 2.75rem;
            overflow-y: hidden;
        }

            .nav-scroller .nav {
                display: flex;
                flex-wrap: nowrap;
                padding-bottom: 1rem;
                margin-top: -1px;
                overflow-x: auto;
                text-align: center;
                white-space: nowrap;
                -webkit-overflow-scrolling: touch;
            }

        .btn-bd-primary {
            --bd-violet-bg: #cff2e4;
            --bd-violet-rgb: 112.520718, 44.062154, 249.437846;
            --bs-btn-font-weight: 600;
            --bs-btn-color: var(--bs-white);
            --bs-btn-bg: var(--bd-violet-bg);
            --bs-btn-border-color: var(--bd-violet-bg);
            --bs-btn-hover-color: var(--bs-white);
            --bs-btn-hover-bg: #cff2e4;
            --bs-btn-hover-border-color: #cff2e4;
            --bs-btn-focus-shadow-rgb: var(--bd-violet-rgb);
            --bs-btn-active-color: var(--bs-btn-hover-color);
            --bs-btn-active-bg: #cff2e4;
            --bs-btn-active-border-color: #cff2e4;
        }

        .bd-mode-toggle {
            z-index: 1500;
        }

            .bd-mode-toggle .dropdown-menu .active .bi {
                display: block !important;
            }

        #myCarousel {
            margin-bottom: 0px;
        }

        .card-deck {
            display: flex;
            flex-wrap: wrap;
            align-items: stretch;
        }

        .card {
            flex: 1 0 auto;
            height: 100%;
        }

        .card-body {
            display: flex;
            flex-direction: column;
        }

        .card-text {
            margin-bottom: 3rem;
        }

        .d-flex {
            margin-top: auto;
        }

        /* Footer Styling */
        footer {
            padding: 5px 0px;
            text-align: center;
            background-color: #286052; /* Dark background color */
            color: #F5F5F5; /* Text color */
        }

            /* Footer Links */
            footer a {
                color: #F5F5F5;
                text-decoration: none;
            }

                footer a:hover {
                    text-decoration: underline;
                }

        /* Bootstrap Styles (for button styling) */
        .btn-primary {
            color: #286052;
            background-color: #F5F5F5;
            border: none;
            padding: 10px 20px;
            cursor: pointer;
            border-radius: 5px;
            transition: background-color 0.3s;
        }

            .btn-primary:hover {
                background-color: #286052;
            }

    </style>

    <!-- Custom styles for this template -->
    <link href="https://getbootstrap.com/docs/5.3/examples/carousel/carousel.css" rel="stylesheet">
</head>
<body class="py-0">
